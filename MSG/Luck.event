#ifndef MODULAR_GETTER_LUCK_EVENT
#define MODULAR_GETTER_LUCK_EVENT

pLckModifiers: // Luck
	POIN prAddUnitLuck prAddEquipLuck prAdd10BecauseLolTest
WORD 0

prAddUnitLuck: // Byte 0x19 in unit struct is Luck
	rAdd(rSByteAt(0x19))

prAddEquipLuck:
	rAdd(rForUnitEquipped(rCallExternal(prGetItemLckBonus)))

prAdd10BecauseLolTest:
	rAddConst(10)

#endif // MODULAR_GETTER_LUCK_EVENT
